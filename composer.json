{
    "name": "durva/ci3-to-ci4-compatible",
    "description": "A package to help migrate CodeIgniter 3 projects to CodeIgniter 4 with additional compatibility features",
    "keywords": ["codeigniter", "CI4","package", "CI3", "compatible"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vipul",
            "email": "mr.vipulmangukiya@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Durva\\CI3ToCI4Compatible\\": "src/CI3ToCI4Compatible"
        },
        "files": [
            "src/CI3ToCI4Compatible/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Durva\\CI3ToCI4Compatible\\Tests\\": "tests//"
        }
    }
}
